{% comment %}Auto-generated placeholder for blog{% endcomment %}
<section class="page-width section-main-blog">
  <h1>{{ blog.title }}</h1>
  {% for article in blog.articles %}
    <article>
      <h2><a href="{{ article.url }}">{{ article.title }}</a></h2>
      <p>{{ article.excerpt }}</p>
    </article>
  {% endfor %}
</section>

{% schema %}{
  "name": "Main Blog",
  "tag": "section",
  "class": "section-main-blog",
  "settings": []
}{% endschema %}
